<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="patient_information_template" inherit_id="acs_hms_base.patient_information_template">
        <xpath expr="//td[@name='origin_div_id']" position="inside">
            <t name="appointment_td" t-if="doc.appointment_id">
                <strong>Origin:</strong> <span t-field="doc.appointment_id"/>
            </t>
        </xpath>
    </template>

</odoo>