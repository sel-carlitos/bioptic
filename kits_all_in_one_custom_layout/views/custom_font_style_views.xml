<odoo>
    <record id="custom_font_tree_view" model="ir.ui.view">
        <field name="name">custom.font.views.tree</field>
        <field name="model">custom.font.style</field>
        <field name="arch" type="xml">
            <list string="Custom Font" editable="bottom">
                <field name='url_date'/>
                <field name="file" filename="file_name" widget="binary"/>
                <field name='file_name' column_invisible='1'/>
            </list>
        </field>
    </record>

    <record id="custom_font_tree_view_actions" model="ir.actions.act_window">
        <field name="name">Font</field>
        <field name="res_model">custom.font.style</field>
        <field name="view_mode">list</field>
        <field name="view_id" ref="custom_font_tree_view"/>
    </record>

    <menuitem name="Font" id="custom_font_style" action="kits_all_in_one_custom_layout.custom_font_tree_view_actions"
              parent="base.menu_administration" sequence="0" groups="base.group_system"/>
</odoo>
