<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_document_form_inherit" model="ir.ui.view">
        <field name="name">ce.document.form.inherit</field>
        <field name="model">l10n_sv.dte.document</field>
        <field name="inherit_id" ref="l10n_sv_dte.l10n_sv_document_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='invoice_id']" position="after">
                <field name="order_id" invisible="not order_id"/>
            </xpath>
		</field>
	</record>
</odoo>
