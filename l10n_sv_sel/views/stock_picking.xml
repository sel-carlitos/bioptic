<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_picking_form_inherit_sel" model="ir.ui.view">
    <field name="name">view.picking.form.inherit.sel</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_form" />
    <field name="arch" type="xml">
      <xpath expr="//header" position="inside">
        <button name="create_delivery_note" type="object" string="Create delivery note"
          class="oe_highlight" invisible="state != 'done'" />
      </xpath>

      <field name="origin" position="after">
        <field name="l10n_sv_dte_id" />
      </field>
      <field name="location_dest_id" position="after">
        <field name="l10n_sv_voucher_type_id"/>

      </field>
    </field>
  </record>
</odoo>
